{
  "name": "df2",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://alexfigliolia.github.io/dogfinder-mongodb-react",
  "devDependencies": {
    "gh-pages": "^0.12.0",
    "node-sass": "^4.5.2",
    "react-scripts": "0.9.5"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "foreman": "^2.0.0",
    "gh-pages": "^0.12.0",
    "jsonp": "^0.2.1",
    "marked": "^0.3.6",
    "mongoose": "^4.9.5",
    "nodemon": "^1.11.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start-dev": "nf start -p 3000",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build&&gh-pages -d build"
  }
}
